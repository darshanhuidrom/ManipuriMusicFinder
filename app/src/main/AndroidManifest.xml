<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kangladevelopers.filmfinder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MoveFinder"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.Setting"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.TestYoutubePlayer"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".Activity.MovieListActivity" />
        <activity
            android:name=".Activity.MovieDetailActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".Activity.ShowBookDetailsActivity" />
        <activity android:name=".Activity.TestActivity" />
        <activity
            android:name=".Activity.Player"
            android:screenOrientation="landscape" />
        <activity android:name=".Activity.MusicDetailActivity" />
        <activity android:name=".Activity.EditProfile" />
        <activity android:name=".Activity.Profile" />
        <activity android:name=".Activity.ListAllActivity" />
        <activity android:name=".Activity.ListActivity" />
        <activity android:name=".Activity.BioDataActivity" />
        <activity android:name=".developers.ui.DeveloperActivity"></activity>
    </application>

</manifest>
